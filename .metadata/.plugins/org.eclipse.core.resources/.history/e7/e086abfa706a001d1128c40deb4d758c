package ex5_poly.model.vo;

public class Member {
	String name;
	int age;
	char gender;
	int couponCount;
	public Member(){
		
	}
	public Member(String name, int age, char gender) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
	}
	@Override
	public String toString() {
		return "Member [name=" + name + ", age=" + age + ", gender=" + gender + ", couponCount=" + couponCount + "]";
	}
	
	
}
